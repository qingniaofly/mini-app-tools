{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "A minimal Electron application tools",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --x64",
    "dev": "yarn main:dev && yarn copy && yarn start",
    "copy": "node ./scripts/copy.js",
    "main:dev": "cd ../pages/main && yarn dev",
    "main:build": "cd ../pages/main && yarn build"
  },
  "author": "yc.tao",
  "repository": {
    "type": "git",
    "url": "https://github.com/qingniaofly/mini-app-tools.git"
  },
  "keywords": [
    "mini-app-tools",
    "node",
    "javascript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qingniaofly/mini-app-tools/issues"
  },
  "build": {
    "appId": "com.yctao.electron.app",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/icon/app.ico"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "public/icon/app.ico"
    }
  },
  "dependencies": {
    "bufferhelper": "^0.2.1",
    "iconv-lite": "^0.6.3",
    "loadsh": "^0.0.4",
    "thirdparty-sdk": "^0.0.1"
  },
  "devDependencies": {
    "electron": "^17.2.0",
    "electron-builder": "^23.0.3"
  }
}
