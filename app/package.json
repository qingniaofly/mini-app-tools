{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --x64",
    "dev": "yarn main:dev && yarn copy && yarn start",
    "copy": "node ./scripts/copy.js",
    "main:dev": "cd ../pages/main && yarn dev",
    "main:build": "cd ../pages/main && yarn build"
  },
  "author": "yc.tao",
  "build": {
    "appId": "com.yctao.electron.app",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/icon/app.ico"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "public/icon/app.ico"
    }
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bufferhelper": "^0.2.1",
    "cheerio": "^1.0.0-rc.10",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.1",
    "iconv-lite": "^0.6.3",
    "loadsh": "^0.0.4",
    "node-fetch": "^3.2.4",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "electron": "^17.2.0",
    "electron-builder": "^23.0.3"
  }
}
